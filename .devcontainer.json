{
  "name": "depth project",
  "dockerFile": "Dockerfile",
  "runArgs": ["--privileged",
    "--gpus", "all",
	  "-e DISPLAY=1",
	  "-e QT_X11_NO_MITSHM=1"
  ],
  "mounts": [
     "source=/dev/bus/usb,target=/dev/bus/usb,type=bind",
     "source=/tmp/.X11-unix,target=/tmp/.X11-unix,type=bind"
 ],
 "extensions": ["c++","cmake","Docker"]
}
